import java.util.Scanner;

public class Hello{
	public static long power(int base, int exp){
		long pow = 1;
		for(int i=1; i<=exp; i++){
			pow = pow*base;
		}
		return pow;
	}
	
	public static void main(String[] args){
		
		Scanner sc = new Scanner(System.in);
		
		int base = sc.nextInt();
		int exp = sc.nextInt();
		
		System.out.println(power(base,exp));
		
	}
}
